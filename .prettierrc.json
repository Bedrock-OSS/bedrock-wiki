{
	"embeddedLanguageFormatting": "off",
	"printWidth": 80,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "es5",
	"useTabs": true,
	"overrides": [
		{
			"files": "*.md",
			"options": {
				"embeddedLanguageFormatting": "auto",
				"parser": "mdx",
				"semi": true,
				"tabWidth": 4,
				"useTabs": false
			}
		}
	]
}
